{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf dist .cache",
    "build:dev": "NODE_ENV=development parcel build src/index.html --no-minify --public-url /dist/",
    "build:prod": "NODE_ENV=production parcel build src/index.html --public-url /dist/",
    "dev:client": "NODE_ENV=development parcel src/index.html",
    "dev": "nodemon --exec \"npm run build:dev\"",
    "start": "npm run dev"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "autoprefixer": "^9.7.4",
    "babel-preset-react-app": "^9.1.0",
    "nodemon": "^2.0.2",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^1.5.0"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "dist/*"
    ],
    "delay": "2000"
  }
}
